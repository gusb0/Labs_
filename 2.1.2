#include <iostream>
#include <vector>

using namespace std;

int main() {
    const int n = 10; // кількість елементів у масивах
    
    // Оголошення трьох масивів індексованого типу
    vector<int> arr1(n); // Перший масив
    vector<int> arr2(n); // Другий масив
    vector<int> arr3(n); // Третій масив

    // Ініціалізація першого масиву згідно з виразом 100 + i * 10
    for (int i = 0; i < n; i++) {
        arr1[i] = 100 + i * 10;
    }

    // Ініціалізація другого масиву згідно з виразом 110 - i
    for (int i = 0; i < n; i++) {
        arr2[i] = 110 - i;
    }

    // Ініціалізація третього масиву рівними значеннями попередніх двох масивів
    for (int i = 0; i < n; i++) {
        arr3[i] = arr1[i] * arr2[i]; // Множимо відповідні елементи
    }

    // Виведення третього масиву
    cout << "Третій масив (добуток відповідних елементів першого та другого масивів):" << endl;
    for (int i = 0; i < n; i++) {
        cout << arr3[i] << " ";
    }
    cout << endl;

    // Знаходження добутку кодів елементів третього масиву
    int product = 1; // Змінна для збереження добутку
    for (int i = 0; i < n; i++) {
        product *= arr3[i];
    }

    // Виведення результату
    cout << "Добуток кодів елементів третього масиву: " << product << endl;

    return 0;
}
