#include <stdio.h>

int main() {
    int a = -1, b = -12, c = 0;
    int d = 94, e = 305, f = -15;
    int g = 537, h = 537, i = 138;

    // Використання операції порівняння ">"
    printf("a > b: %d\n", a > b);  // -1 > -12: очікується 1 (істина)
    printf("c > d: %d\n", c > d);  // 0 > 94: очікується 0 (хибно)
    printf("e > f: %d\n", e > f);  // 305 > -15: очікується 1 (істина)
    printf("g > h: %d\n", g > h);  // 537 > 537: очікується 0 (хибно)
    printf("h > i: %d\n", h > i);  // 537 > 138: очікується 1 (істина)

    return 0;
}
